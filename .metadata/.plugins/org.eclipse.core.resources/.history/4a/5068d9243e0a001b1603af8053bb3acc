
public class Ejemplo2 {

	public static void main(String[] args) {

		
		int num1 = 10, num2 = 20;
		boolean condicion1 = false, condicion2 = true;
		
		boolean resultado = num1 > num2 + num2;
		System.out.println(resultado);
		
		resultado = condicion1 == !condicion2;
		System.out.println(resultado);
		
	}

}

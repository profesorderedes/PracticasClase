
public class Ejemplo8 {

	public static void main(String[] args) {
		
		// Caracteres de control (continuación).
		// \"
		
		System.out.println("En inglés casa se dice \"house\".");
		
		// \'
		System.out.println("En inglés casa se dice \'house\'.");
		
		// \\
		System.out.println("La carpeta donde se almacenan los programas en "
				+ "Windows se llama C:\Archivos de programa");

	}

}
